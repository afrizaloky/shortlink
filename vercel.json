{
    "version": 2,
    "env": {},
    "builds": [{
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "build"
            }
        }, {
            "src": "/build/index.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": ["/build/**"]
            }
        }

    ],
    "routes": [{
            "src": "/(.*)",
            "dest": "/build"
        }

    ]
}